        (
        <foreach collection="list" index="" item="applicationPeople" separator="union all">
            select
            #{applicationPeople.TBSJ},
            #{applicationPeople.QYMC},
            #{applicationPeople.QYTYSHXYDM},
            #{applicationPeople.QYXXDZ},
            #{applicationPeople.QYLXR},
            #{applicationPeople.QYSQFHSJ},
            #{applicationPeople.QYSJ},
            #{applicationPeople.XH},
            #{applicationPeople.XM},
            #{applicationPeople.SFZH},
