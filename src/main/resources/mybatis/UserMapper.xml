            #{applicationPeople.XZD_XSQ},
            #{applicationPeople.XZD_XXDZ},
            #{applicationPeople.XZD_SJ},
            #{applicationPeople.XZD_GWGZ},
            #{applicationPeople.JSY_FGRY},
            #{applicationPeople.CLPZH},
            #{applicationPeople.QYSZDXZHBSHYJ},
            #{applicationPeople.XJLDXZHBJKZM},
            #{applicationPeople.RKWJ},
            #{applicationPeople.WJML},
